package Searching;

public class LinearSearch {
	public static void main(String[] args) {
		int[] a = {20,10,40,25,15};
		int search_ele = 40;
		boolean flag_value = false;
		
		for(int i=0;i<a.length;i++) {
			if(search_ele == a[i]) {
				System.out.println("element found"+ i);
				break;
			}
		}
		if(flag_value= false) {
			System.out.println("Element not found");
		}
	}

}


