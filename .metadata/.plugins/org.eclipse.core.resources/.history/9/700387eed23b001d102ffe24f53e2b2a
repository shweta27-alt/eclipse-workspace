package Numbers;
import java.util.*;

public class Binary_to_Decimal {
	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		int binary = sc.nextInt();
		int n=0;
		int decimal = 0;
		while (binary > 0) {
			int temp = binary%10;
			decimal+=temp*Math.pow(2, n);
			binary= binary/10;
			n++;
		}
		System.out.println("decimal no is" +decimal);
		
	}

}
