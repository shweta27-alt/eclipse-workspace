package lecture_basic;

public class Pair_of_number {
	public static void main(String[] args) {
		int[] num = {2,4,7,9};
		pairs(num);
	}
    
    public static void pairs(int[] num) {
    	for(int i=0;i<num.length;i++) {
    		int current=num[i];
    		for(int j=i+1;j<num.length;j++) {
    			System.out.print("("+current+","+num[j]+")");
    		}
    		System.out.println();
    	}
    }
}
