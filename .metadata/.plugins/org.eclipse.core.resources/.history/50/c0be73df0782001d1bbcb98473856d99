package practice;

public class frequency {
	public static void main(String[] args) {
		int[] arr = {'a','a','b','b','c','c','a','a'};
		int count = 0;
		for(int i=0;i<arr.length;i++) {
			for(int j=i+1;i<arr.length;i++) {
				if(arr[i]==arr[j]) {
					count++;
				}
			}
			
			System.out.println(arr[i]+": "+ count);
		}
	}

}
