package Arrays;
import java.util.*;

public class minimum_scalar_product {
	public static void main(String[] args) {
		int arr1[] = {1, 2, 6, 3, 7};
		int arr2[] = {10, 7, 45, 3, 7};
		int product=1;
		for(int i=0;i<arr1.length;i++) {
			for(int j=0;j<arr2.length;j++) {
				product= arr1[i]*arr2[j];
				arr1[i]++;
				arr2[j]++;
			}
		}
		
		System.out.println(product+" ");
	}

}
